{
  "manifest_version": 3,
  "name": "The Eye",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://*/",
    "http://*/"
  ],
  "background": {
    "service_worker": "serviceWorker.js",
    "type": "module"
  }
}